{
  "name": "v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "concurrently 'yarn start:client' 'yarn start:lambda'",
    "start:client": "netlify dev",
    "start:lambda": "netlify-lambda serve src/lambda",
    "build": "yarn build:client && yarn build:lambda",
    "build:client": "react-scripts build && mv build ../build",
    "build:lambda": "netlify-lambda build src/lambda",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "http-proxy-middleware": "^0.20.0",
    "nodemailer": "^6.4.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.1"
  },
  "devDependencies": {
    "@avarona/react-components": "0.1.8",
    "classnames": "^2.2.6",
    "concurrently": "^5.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "netlify": "^3.0.0",
    "netlify-lambda": "^1.6.3"
  }
}
